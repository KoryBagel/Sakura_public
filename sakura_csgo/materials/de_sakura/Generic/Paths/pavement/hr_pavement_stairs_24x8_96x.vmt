LightmappedGeneric
{
	$basetexture "de_sakura/generic/paths/pavement/stairs_fixer_diffuse"
	$bumpmap "de_sakura/generic/paths/pavement/stairs_fixer_normal"
	$surfaceprop Concrete

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.62 0.62 0.62]"
	$envmapcontrast 1
	$envmapsaturation 0.8
	$envmaplightscale 0.59
	$envmaplightscaleminmax "[0 1]"
	$envmapanisotropy 1
	$envmapanisotropyscale 0.5

	$phong 1
	$phongexponent 195
	$phongamount "[1 1 1 1]"

	Proxies
	{
		AnimatedTexture
		{
			animatedtexturevar "$bumpmap2"
			animatedtextureframenumvar "$bumpframe2"
			animatedtextureframerate 24
		}
	}
}